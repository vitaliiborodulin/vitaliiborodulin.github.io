//begin work bodyclick
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/bodyclick.jpg" alt="BodyClick")
    a(href="https://bodyclick.uz/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title Body Click
    .works-item__date
      +svg('calendar')
      time(datetime='2021-06') june 2021
    a(href="https://bodyclick.uz/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span bodyclick.uz
    .works-item__btns
      .works-item__btn html
      .works-item__btn less
      .works-item__btn smartgrid
      .works-item__btn js
//end work bodyclick
