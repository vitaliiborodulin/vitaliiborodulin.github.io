//begin work museum
.works__item.works-item
  .works-item__img
    img(src="assets/img/works/museum.jpg" alt="museum")
    a(href="https://русскинской-музей.рф/" target="_blank" rel="nofollow noopener noreferrer").works-item__img-more
      +svg('magnific')
      span view site
  .works-item__info
    h3.works-item__title Русскинский музей
    .works-item__date
      +svg('calendar')
      time(datetime='2021-10') october 2021
    a(href="https://русскинской-музей.рф/" target="_blank" rel="nofollow noopener noreferrer").works-item__link
      +svg('link')
      span русскинской-музей.рф
    .works-item__btns
      .works-item__btn smartgrid
      .works-item__btn less
      .works-item__btn js/jquery
      .works-item__btn wordpress
//end work museum
